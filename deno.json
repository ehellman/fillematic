{
  "fmt": {
    "useTabs": false,
    "lineWidth": 120,
    "indentWidth": 2,
    "singleQuote": true,
    "proseWrap": "preserve"
  },
  "tasks": {
    "dev": "deno run --watch main.ts",
    "buy": "deno run --allow-all main.ts"
  },
  "compilerOptions": {
    "lib": [
      "esnext",
      "deno.ns",
      "dom",
      "dom.iterable"
    ]
  },
  "imports": {
    "@std/assert": "jsr:@std/assert@1",
    "puppeteer": "npm:puppeteer@^24.1.1"
  }
}
